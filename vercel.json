{
    "builds": [
      { "src": "package.json", "use": "@vercel/node" }
    ],
    "version": 2,
    "functions": { "api/**/*.js": { "runtime": "nodejs14.x" } },
    "env": {
      "NEXT_PUBLIC_VERCEL_URL": "$VERCEL_URL"
    },
    "build": {
      "env": {
        "VERCEL_URL": "$VERCEL_URL"
      }
    },
    "routes": [
      {
        "src": "/api/webhooks/clerk",
        "dest": "/api/webhooks/clerk.js"
      }
    ]
  }
  